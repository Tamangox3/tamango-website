---
import type { TypePageSkeleton } from "@lib/generated/contentful";
import type { Entry } from "@lib/types";
import Base from "./Base.astro";
import MiamiScroll2 from "@components/miami/miami-2.astro";

interface Props extends Entry<TypePageSkeleton> {}

const page = Astro.props;

const MiamiComponent = MiamiScroll2;

const props = { ...page, introAnimation: false };
---

<Base {...props}>
  <MiamiComponent />
</Base>
