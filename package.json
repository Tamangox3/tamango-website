    "astro-robots-txt": "^1.0.0",
    "contentful": "^11.1.4",
    "gsap": "^3.12.5",
    "howler": "^2.2.4",
    "lottie-web": "^5.12.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/howler": "^2.2.12",
